{
  "name": "gatsby-starter-storybook",
  "description": "Gatsby Storybook Starter",
  "version": "1.1.0",
  "author": "Marko Radak <mail@markoradak.com>",
  "license": "MIT",
  "engines": {
    "node": ">=0.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markoradak/gatsby-starter-storybook"
  },
  "keywords": [
    "gatsby",
    "storybook",
    "styled-components",
    "reset-css",
    "eslint"
  ],
  "pre-commit": [
    "test",
    "format"
  ],
  "scripts": {
    "start": "gatsby develop -H 0.0.0.0 -o",
    "test": "node --experimental-modules ./src/test/test.yml.js && node ./src/test/test.seo.js && node ./src/test/test.blog.js && node ./src/test/test.course.js && node ./src/test/test.landing.js && node ./src/test/test.location.js && node ./src/test/test.downloadable.js",
    "start:storybook": "start-storybook -p 9000 -c .storybook",
    "build": "npm run test && node ./src/utils/makePagesDictionary.js && node ./src/utils/makePostsDictionary.js && node ./src/utils/makeRedirectsDictionary.js && gatsby build",
    "build:gatsby": "gatsby build",
    "serve": "gatsby serve -o -p 8080",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "open:e2e": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start http://localhost:8000 cy:open",
    "run:e2e": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start http://localhost:8000 cy:run",
    "cy:clean": "cypress cache clear",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@loadable/component": "^5.13.2",
    "browser-monads": "^1.0.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.8.29",
    "gatsby": "^2.24.2",
    "gatsby-background-image": "^1.1.2",
    "gatsby-image": "^2.4.14",
    "gatsby-plugin-gdpr-cookies": "^1.0.7",
    "gatsby-plugin-google-tagmanager": "^2.1.25",
    "gatsby-plugin-image": "^1.6.0",
    "gatsby-plugin-loadable-components-ssr": "^2.1.0",
    "gatsby-plugin-manifest": "^2.2.14",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-offline": "^3.0.2",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-remove-console": "^0.0.2",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-rollbar": "^1.1.0",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sharp": "^2.14.4",
    "gatsby-plugin-sitemap": "^2.2.27",
    "gatsby-plugin-styled-components": "^3.1.4",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-plugin-zeit-now": "^0.2.0",
    "gatsby-remark-autolink-headers": "^4.10.0",
    "gatsby-remark-component": "^1.1.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.3.27",
    "gatsby-remark-lazy-image-url": "^1.0.1",
    "gatsby-remark-lazy-load": "^1.0.2",
    "gatsby-remark-reading-time": "^1.1.0",
    "gatsby-source-filesystem": "^2.1.21",
    "gatsby-transformer-remark": "^2.6.32",
    "gatsby-transformer-sharp": "^2.12.1",
    "gatsby-transformer-yaml": "^2.2.15",
    "lazysizes": "^5.2.2",
    "node-fetch": "^2.6.1",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-cookiebot": "^1.0.10",
    "react-dom": "^16.12.0",
    "react-google-charts": "^3.0.15",
    "react-helmet": "^6.0.0-beta",
    "react-input-mask": "^2.0.4",
    "react-intersection-observer": "^8.26.1",
    "react-lazy-youtube": "^1.0.1",
    "react-lazyload": "^3.0.0",
    "react-player": "^1.14.2",
    "react-responsive-carousel": "^3.1.51",
    "react-scrollspy": "^3.4.2",
    "react-select": "^3.1.1",
    "react-share": "^4.1.0",
    "react-simple-code-editor": "^0.11.0",
    "react-slick": "^0.28.1",
    "react-switch": "^5.0.1",
    "react-trend": "^1.2.5",
    "react-twitter-embed": "^3.0.3",
    "rehype-react": "^7.0.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^4.3.2",
    "styled-reset-advanced": "^1.0.1",
    "workerize-loader": "^1.3.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-notes": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "colors": "^1.4.0",
    "cypress": "^8.0.0",
    "cypress-terminal-report": "^3.2.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.14.3",
    "front-matter": "^4.0.2",
    "gatsby-plugin-webpack-size": "^1.0.0",
    "jest": "^25.5.3",
    "js-yaml": "^3.13.1",
    "markdown-link-extractor": "^3.0.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "simple-git": "^2.2.0",
    "start-server-and-test": "^1.13.1",
    "yaml": "^1.7.2",
    "yarn-run-all": "^3.1.1"
  }
}
